package vo;

public class Member {

	private int memberNum;
	private String id;
	private String password;
	private String email;
	private String date;

	public Member() {
	}

	public Member(String id, String password, String email, String date) {
		this.id = id;
		this.password = password;
		this.email = email;
		this.date = date;
	}

	public Member(int memberNum, String id, String password, String email, String date) {
		this.memberNum = memberNum;
		this.id = id;
		this.password = password;
		this.email = email;
		this.date = date;
	}

	public int getMemberNum() {
		return memberNum;
	}

	public void setMemberNum(int memberNum) {
		this.memberNum = memberNum;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getPassword() {
		return password;
	}

	public void setPassword(String password) {
		this.password = password;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public String getDate() {
		return date;
	}

	public void setDate(String date) {
		this.date = date;
	}

	@Override
	public String toString() {
		return "Member [memberNum=" + memberNum + ", id=" + id + ", password=" + password + ", email=" + email
				+ ", date=" + date + "]";
	}

}
